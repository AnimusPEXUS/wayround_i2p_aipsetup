clisp() {
    PACKAGENAME=
    PACKAGENAMESUFIX="\`date '+%Y%m%d%H%M%S'\`-i486-pc-linux-gnu"
    SEPARATEDIR=1
    RELATIVE='.'
    
    local j=0
    RUN[$j]='configure --prefix=/usr --cbc --srcdir=$bs_run_dir --host=i486-pc-linux-gnu'
    RELATIVELY[$j]=1
    ERRORMESSAGE[$j]='*** some configure script error'
    EXITONERROR[$j]=1

}
